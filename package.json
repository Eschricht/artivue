{
  "name": "artivue",
  "type": "module",
  "version": "0.3.12",
  "private": true,
  "packageManager": "pnpm@8.14.2",
  "description": "Say goodbye to the endless cycle of picking shades like bg-dark-200 and hello to a world where creating the perfect color is a breeze",
  "author": "Eschricht <https://github.com/eschricht>",
  "license": "MIT",
  "homepage": "https://artivue.eschricht.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eschricht/unocss-preset-hsl.git"
  },
  "bugs": {
    "url": "https://github.com/Eschricht/unocss-preset-hsl/issues"
  },
  "keywords": [
    "css",
    "unocss",
    "unocss-preset",
    "unocss-preset-hsl",
    "hsl",
    "theming",
    "styling",
    "colors"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "tsx ./scripts/build.ts && pnpm -r --filter=./packages/nuxt dev:prepare",
    "clean": "rimraf packages/*/dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "ts-check": "vue-tsc --noEmit",
    "docs:preview": "vitepress preview docs",
    "docs:build": "tsx ./scripts/build.ts && vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "release": "bumpp -r"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/node": "^20.11.16",
    "@unocss/eslint-plugin": "^0.58.5",
    "@vue/tsconfig": "^0.5.1",
    "bumpp": "^9.3.0",
    "changelogen": "^0.5.5",
    "eslint": "^9.7.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.7",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitepress": "1.0.0-rc.45",
    "vue-tsc": "^2.0.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9",
        "@typescript-eslint/parser": "7"
      }
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
