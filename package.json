{
  "name": "unocss-preset-hsl",
  "version": "0.0.2",
  "packageManager": "pnpm@8.14.2",
  "description": "Say goodbye to the endless cycle of picking shades like bg-dark-200 and hello to a world where creating the perfect color is a breeze",
  "author": "Eschricht <https://github.com/eschricht>",
  "license": "MIT",
  "homepage": "https://github.com/Eschricht/unocss-preset-hsl/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eschricht/unocss-preset-hsl.git"
  },
  "bugs": {
    "url": "https://github.com/Eschricht/unocss-preset-hsl/issues"
  },
  "keywords": [
    "css",
    "unocss",
    "unocss-preset",
    "unocss-preset-hsl",
    "hsl",
    "theming",
    "styling",
    "colors"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev:example": "pnpm -C examples/vite-vue dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "ts-check": "vue-tsc --noEmit",
    "release": "bumpp package.json"
  },
  "peerDependencies": {
    "@unocss/preset-mini": ">= 0.56.0 < 1",
    "unocss": ">= 0.56.0 < 1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/node": "^20.11.16",
    "@unocss/core": ">= 0.58.4 < 1",
    "@unocss/eslint-config": "^0.58.4",
    "@unocss/preset-mini": ">= 0.58.4 < 1",
    "bumpp": "^9.3.0",
    "eslint": "npm:eslint-ts-patch@8.56.0-0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.56.0-0",
    "std-env": "^3.7.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  }
}
